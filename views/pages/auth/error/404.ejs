<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    
    <title>:: Oreo RealEstate :: Sign In</title>
    <!-- Favicon-->
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    
    <!-- Bootstrap Css CDN -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <!-- Custom Css -->
    <% if(typeof(styles) != "undefined" && styles !== null){  %>
        <% styles.forEach(function(styleLink) { %>
           <link rel="stylesheet" href="<%= host+styleLink %>">
        <% }); %>
    <% } %>
    <script src="https://cdn.jsdelivr.net/npm/vue@2.6.11"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.0/axios.min.js"></script>
    <script src="<%= host %>/javascript/auth.js"></script>
</head>

<body class="theme-purple authentication sidebar-collapse">

<% include ../layout/nav %>
   
    <div class="page-header" id="error_404_section">
        <div class="page-header-image" style="background-image:url(<%= host %>/assets/images/login.jpg)"></div>
        <div class="container">
            <div class="col-md-12 content-center">
                <div class="card-plain">
                    <form class="form" method="" action="#">
                        <div class="header">
                            <div class="logo-container">
                                <img src="<%= host %>/assets/images/icons/logo.svg" alt="">
                            </div>
                            <h5>Error 404</h5>
                            <span>Page not found</span>
                        </div>
                        <div class="content">
                            <div class="input-group">
                                <input type="text" class="form-control" placeholder="Search...">
                                <span class="input-group-addon">
                                    <i class="zmdi zmdi-search"></i>
                                </span>
                            </div>
                        </div>
                        <div class="footer text-center">
                            <a href="<%= host %>/login" class="btn btn-primary btn-round btn-lg btn-block waves-effect waves-light">GO TO HOMEPAGE</a>
                            <h5><a href="javascript:void(0);" class="link">Need Help?</a></h5>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container">
                <div class="copyright">
                    &copy;
                    <script>
                        document.write(new Date().getFullYear())
                    </script>,
                    <span>Designed by <a href="https://amanagroupbd.com" target="_blank">Amanagroup</a></span>
                </div>
            </div>
        </footer>
    </div>
    
